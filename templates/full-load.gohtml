<section class="advisory">
  <div class="container">
    <div class="row">
      <div class="col-md-10">
        <h2>Is there something you want to ask?</h2>
      </div>
      <div class="col-md-2">
        <a href="/contact-us" class="btn btn-primary">CONTACT US<i class="fa fa-envelope"></i></a>
      </div>
    </div>
  </div>
</section>


<div class="remodal" data-remodal-id="contact-us" role="dialog" aria-labelledby="modal1Title" aria-describedby="modal1Desc">
  <button data-remodal-action="close" class="remodal-close" aria-label="Close"></button>
  <div>
    <h2 id="modal1Title">CONTACT US</h2>
    <br><br>
    <p id="modal1Desc">
      <form action="#" method="post" id="contactModal">

        <div class="row">
          <div class="col-md-6">
            <input class="form-control" type="text" name="name" id="cmname" value="" size="40" aria-required="true" aria-invalid="false" placeholder="Name">
          </div>

          <div class="col-md-6">
            <input class="form-control" type="tel" name="phone" id="cmphone" value="" size="40" aria-required="true" aria-invalid="false" placeholder="Phone">
          </div>


        </div>

        <div class="row">
          <div class="col-md-6">
            <br>
            <input class="form-control" type="email" name="email" id="cmemail" value="" size="40" aria-required="true" aria-invalid="false" placeholder="Email">
            <br>
            <input class="form-control" type="text" name="subject" id="cmsubject" value="" size="40" aria-required="true" aria-invalid="false" placeholder="Subject">
          </div>
          <div class="col-md-6">
            <br>
            <textarea class="form-control" name="message" id="cmmessage" cols="40" rows="4" aria-invalid="false" placeholder="Message"></textarea>
          </div>
        </div>
<br><br>
      <input class="btn btn-primary float-right" id="csubmit" type="submit" value="SEND MESSAGE">







      </form>
      <br>
      <span id="cerror"> Please fill out all of the fields.</span>
    </p>
  </div>
  <br>
  <!-- <button data-remodal-action="cancel" class="remodal-cancel">Cancel</button> -->

</div>
{{template "footer"}}
